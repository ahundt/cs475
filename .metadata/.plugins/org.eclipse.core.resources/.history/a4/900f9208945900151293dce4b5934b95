package cs475;

import java.io.Serializable;

public class Instance implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = 7363889964150800372L;
	Label _label = null;
	FeatureVector<Integer,Double> _feature_vector = null;

	public Instance(FeatureVector<Integer,Double> feature_vector, Label label) {
		this._feature_vector = feature_vector;
		this._label = label;
	}

	public Label getLabel() {
		return _label;
	}

	public void setLabel(Label label) {
		this._label = label;
	}

	public FeatureVector<Integer,Double> getFeatureVector() {
		return _feature_vector;
	}

	public void setFeatureVector(FeatureVector<Integer,Double> feature_vector) {
		this._feature_vector = feature_vector;
	}
	
	
}
